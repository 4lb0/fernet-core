{
    "name": "fernet/framework",
    "description": "The Fernet Framework",
    "type": "library",
    "keywords": ["framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigo 'Albo' Arce",
            "email": "albo@pragmore.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "symfony/http-foundation": "^5.2",
        "monolog/monolog": "^2.2",
        "league/container": "^3.3",
        "vlucas/phpdotenv": "^5.2",
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.8"
    },
    "autoload": {
        "psr-4": {"Fernet\\": "src/Fernet"}
    },
    "autoload-dev": {
        "psr-4": {"Fernet\\Tests\\": "tests/Fernet/Tests"}
    },
    "minimum-stability": "dev",
    "prefer-stable": true        
}
